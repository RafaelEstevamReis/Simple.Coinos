<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.1</TargetFramework>
		<LangVersion>12</LangVersion>
		<Nullable>enable</Nullable>
		<ImplicitUsings>disable</ImplicitUsings>
		<Version>0.9.3</Version>
		<Authors>RafaelEstevam.net</Authors>
		<Company>Rafael Estevam</Company>
		<Description>A simple Coinos.io C# API client</Description>
		<PackageProjectUrl>https://github.com/RafaelEstevamReis/Simple.Coinos</PackageProjectUrl>
		<RepositoryUrl>https://github.com/RafaelEstevamReis/Simple.Coinos.git</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageTags>Coinos.IO API</PackageTags>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageReleaseNotes>https://github.com/RafaelEstevamReis/Simple.Coinos</PackageReleaseNotes>
		<PackageReadmeFile>README.md</PackageReadmeFile>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\README.md">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>
	
	<ItemGroup>
		<PackageReference Include="Simple.API" Version="1.9.8.3" />
	</ItemGroup>

</Project>
